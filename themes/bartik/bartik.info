stylesheets[print][] = css/print.css
stylesheets[all][] = css/jquery.fancybox-1.3.4.css
stylesheets[all][] = css/general.css

stylesheets[(max-width: 479px)][] = css/responsive-mobile-portrait-layout.css
stylesheets[(min-width: 480px)  and (max-width: 767px)][] = css/responsive-mobile-landscape-layout.css
stylesheets[(min-width: 768px)  and (max-width: 979px)][] = css/responsive-tablet-layout.css
